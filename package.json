{
  "name": "react-dfp-slot",
  "version": "0.0.9",
  "description": "React based DFP library",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn run clean && yarn run build:js",
    "build:js": "babel src/ -d lib/",
    "build:css": "node-sass src/style.scss lib/style.css --output-style compressed",
    "clean": "rimraf lib",
    "lint-staged": "lint-staged",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "git add",
      "eslint ."
    ],
    "*.scss": [
      "sass-lint --max-warnings 0 -v"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hwrdy/react-dfp-slot.git"
  },
  "author": "Hwrdy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hwrdy/react-dfp-slot/issues"
  },
  "homepage": "https://github.com/Hwrdy/react-dfp-slot#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.2",
    "node-sass": "^4.5.3",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.4.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "lodash.debounce": ">= 4.0.3",
    "prop-types": ">= 15.5.10",
    "react-waypoint": ">= 7.0.0"
  },
  "keywords": [
    "ad unit",
    "adunit",
    "advertising",
    "babel",
    "dfp",
    "doubleclick",
    "es6",
    "gpt",
    "google dfp",
    "react",
    "react dfp",
    "react-component"
  ],
  "engines": {
    "node": ">=4.2.4"
  }
}
